<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RightMapFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/mapview"
            android:layout_width="690dp"
            android:layout_height="460dp"
            android:background="@drawable/redright">


            <Button
                android:id="@+id/cargobutton1_right"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignStart="@+id/cargobutton5_right"
                android:layout_alignTop="@+id/cargobutton2_right"
                android:layout_marginStart="0dp"
                android:layout_marginTop="-12dp"
                android:text="Button" />

            <Button
                android:id="@+id/cargobutton2_right"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignTop="@+id/cargobutton3_right"
                android:layout_alignParentStart="true"
                android:layout_marginStart="201dp"
                android:layout_marginTop="0dp"
                android:text="Button" />

            <Button
                android:id="@+id/cargobutton3_right"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignTop="@+id/cargobutton4_right"
                android:layout_alignParentStart="true"
                android:layout_marginStart="106dp"
                android:text="Button" />

            <Button
                android:id="@+id/cargobutton4_right"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="-99dp"
                android:layout_marginBottom="152dp"
                android:layout_toStartOf="@+id/cargobutton7_right"
                android:text="Button" />

            <Button
                android:id="@+id/cargobutton5_right"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="169dp"
                android:layout_marginEnd="280dp"
                android:text="Button" />

            <Button
                android:id="@+id/cargobutton6_right"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignTop="@+id/cargobutton8_right"
                android:layout_toEndOf="@+id/cargobutton3_right"
                android:text="Button" />

            <Button
                android:id="@+id/cargobutton7_right"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignTop="@+id/cargobutton6_right"
                android:layout_alignParentStart="true"
                android:layout_marginStart="98dp"
                android:text="Button" />

            <Button
                android:id="@+id/cargobutton8_right"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="154dp"
                android:text="Button" />

            <Button
                android:id="@+id/button9_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="143dp"
                android:text="Button"
                android:visibility="invisible" />

            <Button
                android:id="@+id/button10_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:text="Button" />

            <Button
                android:id="@+id/button11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="142dp"
                android:text="Button"
                android:visibility="invisible" />

            <Button
                android:id="@+id/button12_right"
                android:layout_width="wrap_content"
                android:layout_height="212dp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="104dp"
                android:text="Button"
                android:visibility="invisible" />


        </RelativeLayout>
    </LinearLayout>

</FrameLayout>