<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LeftMapFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/mapview"
            android:layout_width="690dp"
            android:layout_height="460dp"
            android:background="@drawable/jankredleft1">

            <ImageView
                android:id="@+id/cargoimage1_left"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignParentTop="true"
                android:layout_alignStart="@+id/cargoimage5_left"
                android:layout_marginTop="175dp"
                android:text="1" />

            <ImageView
                android:id="@+id/cargoimage2_left"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:layout_marginEnd="202dp"
                android:layout_marginTop="158dp"
                android:text="2" />

            <ImageView
                android:id="@+id/cargoimage3_left"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignParentEnd="true"
                android:layout_alignTop="@+id/cargoimage2_left"
                android:layout_marginEnd="102dp"
                android:text="3" />

            <ImageView
                android:id="@+id/cargoimage4_left"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignParentEnd="true"
                android:layout_alignTop="@+id/cargoimage2_left"
                android:text="4" />

            <ImageView
                android:id="@+id/cargoimage5_left"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignParentStart="true"
                android:layout_alignTop="@+id/cargoimage8_left"
                android:layout_marginStart="284dp"
                android:text="5" />

            <ImageView
                android:id="@+id/cargoimage6_left"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignParentBottom="true"
                android:layout_alignStart="@+id/cargoimage2_left"
                android:layout_marginBottom="153dp"
                android:text="6" />

            <ImageView
                android:id="@+id/cargoimage7_left"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignStart="@+id/cargoimage3_left"
                android:layout_alignTop="@+id/cargoimage6_left"
                android:text="ImageView" />

            <ImageView
                android:id="@+id/cargoimage8_left"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignParentEnd="true"
                android:layout_alignTop="@+id/cargoimage6_left"
                android:text="ImageView" />

            <ImageView
                android:id="@+id/button9_left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="12dp"
                android:layout_marginTop="144dp"
                android:text="ImageView"
                android:visibility="visible" />

            <ImageView
                android:id="@+id/button11_left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignStart="@+id/button9_left"
                android:layout_marginBottom="141dp"
                android:text="ImageView"
                android:visibility="visible" />

            <ImageView
                android:id="@+id/button12_left"
                android:layout_width="wrap_content"
                android:layout_height="212dp"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="109dp"
                android:text="ImageView"
                android:visibility="visible" />

            <ImageView
                android:id="@+id/button10_left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/button9_left"
                android:layout_centerVertical="true"
                android:text="ImageView" />


        </RelativeLayout>
    </LinearLayout>

</FrameLayout>